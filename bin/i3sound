#!/usr/bin/env python
# -*- coding: utf-8 -*-

import subprocess
import re

cmd = 'amixer sget Master'
process = subprocess.Popen(cmd.split(' '), stdout=subprocess.PIPE)
output, error = process.communicate()
volume_full = output.split()[-3]
result = int(re.sub('[^0-9]','', volume_full))

if result > 66:
    print('ğŸ”Š')
elif result > 33:
    print('ğŸ”‰')
else:
    print('ğŸ”ˆ')
